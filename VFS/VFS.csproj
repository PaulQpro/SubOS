    <None Remove="bin\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="VFS.cs">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
  </ItemGroup>

</Project>
